/**index.wxss**/
.top{
  display: flex;
  align-self: center;
  justify-content: center;
  align-items: center;
  background-color: #F0F0F0;
  height: 180px;
  border-bottom: 4px solid #F0F0F0;
}
.bkg{
  width: 100%;
  height:180px;
  margin-top: 7%;
}
.all{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 7%;
  border-top: 7px solid #F0F0F0;
  padding-top: 5%;
  
}
.div{
  display: flex;
  align-self: center;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}
.div_part{
  display: flex;
  align-self: center;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin: 2% 10% 2% 10%;
}
.icon{
  height: 80px;
  width: 80px;
}
.function_font{
  font-size: 20px;
  font-family: 'Noto Sans S Chinese DemiLight';
  
}
.pic_div{
  display: flex;
  align-self: center;
  justify-content: center;
  align-items: center;
  height: 110px;
  width: 110px;
  border-top-left-radius: 10%;
  border-bottom-left-radius: 10%;
  border-top-right-radius: 10%;
  border-bottom-right-radius: 10%;
  border: 3px solid 	#F0F0F0;
  margin-bottom: 4%;
}
.line{
  margin-top: 5%;
  background-color: #F0F0F0;
  height: 10px;
}
